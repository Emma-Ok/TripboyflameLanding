    badgeText="ESCUCHALO AHORA MISMO EN TODAS LAS PLATAFORMAS."
    title="Baby Glissy - Majo Navideño ( Video Oficial ) prod. Yxng divi"
    subtitle={["FILM BY: 2 PORCIENTO",
      "ARTE: DAO",
      "EDIT: LUY TRIPBOYFLAME",
      "VFX: ITSLILJAY.RAW",
      "WAP❄️H",
      "__LAS TORRES__"]}
  />

  <InfographicBody images={[
    "/collage-info/pikachu.webp",
    "/collage-info/pikachumodal.webp",
    "/collage-info/beretta.webp",
  ]}  />
</section>