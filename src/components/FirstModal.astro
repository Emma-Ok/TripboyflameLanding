    subtitle={["FILM BY: LUY TRIPBOYFLAME",
      "ARTE: TOM_ARTZ",
      "EDIT: LUY TRIPBOYFLAME",
      "VFX: VAIRROW",
      "PRENDELO HAINY 🗣️",
      ""]}
  />

  <InfographicBody images={[
    "https://tripboyflamebucket.s3.us-west-1.amazonaws.com/modals/modal1/gorillablue1.webp",
    "https://tripboyflamebucket.s3.us-west-1.amazonaws.com/modals/modal1/gorillablue7.webp",
    "https://tripboyflamebucket.s3.us-west-1.amazonaws.com/modals/modal1/gorillablue3.webp",
    "https://tripboyflamebucket.s3.us-west-1.amazonaws.com/modals/modal1/gorillablue4.webp",
    "https://tripboyflamebucket.s3.us-west-1.amazonaws.com/modals/modal1/gorillablue5.webp",
