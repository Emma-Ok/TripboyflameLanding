{
  "extends": "astro/tsconfigs/strict",
  
  
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@lib/*": ["src/lib/*"],
      "@icons/*": ["src/icons/*"],
      "@ui/*": ["src/components/ui/*"],
      "@types" : ["src/types.ts"]
    }
  }
}
